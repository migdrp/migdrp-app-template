{
	"name": "migdrp-app-template",
	"version": "1.0.0",
	"description": "Default application tempate for migdrp",
	"engines": {
		"node": "14.x",
		"npm": "6.x"
	},
	"dependencies": {
		"express": "^4.17.1"
	},
	"scripts": {
		"dev": "set NODE_ENV=development&& npx run-p wp:dev serve",
		"prod": "set NODE_ENV=production&& npx run-s wp:build serve",
		"serve": "ts-node ./src/backend/server.ts",
		"wp:dev": "webpack -w --config webpack.config.ts",
		"wp:build": "webpack --config webpack.config.ts"
	},
	"browserslist": "> 0.25%, not dead",
	"author": "migdrp <mig_drp@hotmail.com>",
	"license": "SEE LICENSE IN LICENSE.md",
	"devDependencies": {
		"@babel/core": "^7.14.8",
		"@babel/preset-env": "^7.14.8",
		"@babel/preset-typescript": "^7.14.5",
		"@types/express": "^4.17.13",
		"@types/node": "^14.17.6",
		"@types/webpack": "^5.28.0",
		"@types/webpack-dev-server": "^3.11.5",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"babel-loader": "^8.2.2",
		"dotenv": "^10.0.0",
		"html-webpack-plugin": "^5.3.2",
		"npm-run-all": "^4.1.5",
		"ts-loader": "^7.0.5",
		"ts-node": "^9.1.1",
		"typescript": "^4.3.5",
		"webpack": "^5.47.0",
		"webpack-cli": "^4.7.2"
	}
